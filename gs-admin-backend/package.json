{
  "name": "gaming-backend",
  "version": "1.0.0",
  "description": "Backend API",
  "main": "main.js",
  "license": "ISC",
  "scripts": {
    "watch:docker": "npm run db:init && npm run search:init && nodemon ./bin/index.js",
    "db:init": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "search:init": "node ./bin/reindex-search.js",
    "start": " node ./dist/bin",
    "dev": "NODE_ENV=development babel-node ./bin",
    "prod": "NODE_ENV=production  node ./dist/bin",
    "build": "babel --config-file ./.babelrc . -d ./dist  && cp ./swagger.json ./dist/",
    "babel-node": "babel-node --inspect=0.0.0.0:9229",
    "clean": "rimraf dist",
    "watch": "./node_modules/.bin/nodemon ./bin",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx sequelize db:create && npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "seed": "npx sequelize db:seed:all",
    "lint": "npx prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.267.0",
    "@opensearch-project/opensearch": "^2.2.0",
    "@sendgrid/mail": "7.7.0",
    "amqplib": "0.10.3",
    "aws-sdk": "2.1104.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "1.1.3",
    "bcrypt": "5.0.1",
    "cli-progress": "^3.11.2",
    "convict": "6.1.0",
    "cors": "2.8.5",
    "crypto-js": "^4.1.1",
    "currency-symbol-map": "5.1.0",
    "customerio-node": "3.3.6",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-boom": "3.0.0",
    "js-yaml": "4.1.0",
    "json-crypto": "1.0.5",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "multer": "1.4.4",
    "node-cron": "3.0.1",
    "node-fetch": "2.6.1",
    "node-schedule": "2.1.0",
    "node-xlsx": "^0.21.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.6.0",
    "pg-hstore": "2.3.4",
    "prettier": "^2.8.4",
    "reflect-metadata": "0.1.13",
    "sequelize": "5.21.8",
    "sharp": "0.30.4",
    "swagger-ui-express": "4.6.0",
    "validate.js": "0.13.1",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/register": "7.15.3",
    "babel-eslint": "10.0.3",
    "eslint": "7.29.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "nodemon": "2.0.15",
    "npx": "^10.2.2",
    "sequelize-cli": "^5.4.0"
  }
}
